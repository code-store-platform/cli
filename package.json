{
  "name": "codestore-cli",
  "description": "Command Line Interface of code.store. Add services, deploy, debug, perform all operations from your terminal.",
  "version": "1.0.0",
  "bin": {
    "codestore": "./bin/run",
    "cs": "./bin/run"
  },
  "bugs": "https://github.com/code-store-marketplace/codestore-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.3",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "apollo-boost": "^0.4.7",
    "axios": "^0.19.2",
    "chalk": "^3.0.0",
    "cli-ux": "^5.4.5",
    "cross-fetch": "^3.0.4",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "inquirer": "^7.1.0",
    "jest": "^25.2.7",
    "node-config-ts": "^3.0.4",
    "open": "^7.0.3",
    "ora": "^4.0.3",
    "os": "^0.1.1",
    "ts-jest": "^25.3.1",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.10",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.17",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.26.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "globby": "^10.0.2",
    "jest-runner-eslint": "^0.7.6",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/code-store-marketplace/codestore-cli",
  "keywords": [
    "codestore",
    "cli",
    "command line interface",
    "reuse"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "codestore",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "context": {
        "description": "Manage global Project and Service contexts"
      }
    }
  },
  "repositâ€œory": "code-store-marketplace/codestore-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.js",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest --coverage --runInBand --detectOpenHandles",
    "version": "oclif-dev readme && git add README.md",
    "start": "node ./bin/run"
  },
  "types": "lib/index.d.ts",
  "jest": {
    "projects": [
      {
        "displayName": "lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/src/**/*.ts"
        ],
        "rootDir": "src"
      },
      {
        "displayName": "test",
        "moduleFileExtensions": [
          "js",
          "json",
          "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
          "^.+\\.ts$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "collectCoverage": true,
        "coveragePathIgnorePatterns": [
          "/node_modules/",
          "/dist/",
          ".dto.ts$"
        ],
        "testEnvironment": "node"
      }
    ]
  }
}
