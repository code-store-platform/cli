{
  "name": "code-store",
  "version": "0.0.0",
  "author": "Denis Mukhopadov",
  "bin": {
    "cs": ".\\bin\\run"
  },
  "bugs": "https://github.com/cli/code-store/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/rimraf": "^2.0.3",
    "chalk": "^3.0.0",
    "inquirer": "^7.0.0",
    "oclif": "^1.14.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^12.12.7",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "rimraf": "^3.0.0",
    "ts-node": "^8",
    "tslint": "^5.20.1",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/cli/code-store",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "cli/code-store",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "start": "node .\\bin\\run"
  },
  "types": "lib/index.d.ts"
}
